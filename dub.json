    "description": "Simple toy operating system.",
    "license": "",
    "name": "dinitos",
    "configurations": [
        {
            "name": "riscv32",
            "versions": [
                "Qemu",
                "Riscv32",
                "Riscv32Debug",
                "FeatureFloatPoint"
            ],
            "dflags-ldc": [
                "--mtriple=riscv32-unknown-elf",
                "--unittest",
                "--mattr=+m,+a,+c,+f",
                "--mabi=ilp32f"
            ],
            "buildRequirements": ["allowWarnings"]
        },
        {
            "name": "riscv32-release",
            "versions": [
                "Qemu",
                "Riscv32",
                "FeatureFloatPoint"
            ],
            "dflags-ldc": [
                "-mattr=+m,+a,+c,+f",
                "-mtriple=riscv32-unknown-elf",
                "-Oz",
                "--mabi=ilp32f",
                "-L-Wl,--gc-sections",
                "--function-sections",
                "--data-sections"
            ]
        },
        {
            "name": "riscv64",
            "versions": [
                "Qemu",
                "Riscv64",
                "Riscv64Debug",
                "FeatureFloatPoint",
                "PTS64"
            ],
            "dflags-ldc": [
                "-mtriple=riscv64-unknown-none",
                "--unittest",
                "-mattr=+m,+a,+c,+f,+d",
                "--mabi=lp64d"
            ]
        },
        {
            "name": "riscv64-release",
            "versions": [
                "Qemu",
                "Riscv64",
                "FeatureFloatPoint",
                "PTS64"
            ],
            "dflags-ldc": [
                "-mtriple=riscv64-unknown-elf",
                "-Oz",
                "-mattr=+m,+a,+c,+f,+d",
                "--mabi=lp64d"
            ]
        }
    ],
    "dflags-ldc": [
        "-betterC",
        "-dip1000",
        "-preview=in",
        "--disable-simplify-drtcalls",
        "--disable-simplify-libcalls",
        "--nodefaultlib",
        "--fvisibility=hidden",
        "--fno-delete-null-pointer-checks",
        "--fno-plt",
        "--code-model=medium",
        "--relocation-model=static"
    ],
    "targetType": "library",
    "targetName": "kernel",
    "targetPath": "./build",
    "sourcePaths": [
        "./src"
    ],
    "importPaths": [
        "./src"
    ],
    "dependencies": {}
}